{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/realm_locations_sold.csv",
      "dest": "/public/realm_locations_sold.csv"
    },
    {
      "src": "/src/app.js",
      "dest": "/src/app.js"
    }
  ]
}
